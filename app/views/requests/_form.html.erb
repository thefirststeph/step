<%= form_for @request do |f| %>
  <%= f.hidden_field :patient_id, value: patient.id %>
  <p>
    <%= f.label :patient_id %>
    <%= f.collection_select :patient_id, organization.patients, :name %>
  </p>
  
  <p> 
    <%= f.label :location %><br>
    <%= f.text_field :location %>
  </p>

  <p> 
    <%= f.label :time %><br>
    <%= f.date_field :time %>
  </p>

  <p> 
    <%= f.label :notes %><br>
    <%= f.text_area :notes %>
  </p>
  <%= f.label :issue_param %>
  <%= f.select :issue_param, ['option1', 'option2']%>
  <%= f.select :desired_attribute, ['option1', 'option2']%>
  <%= f.label :client_language_param %>
  <%= f.select :client_language_param, ['option1', 'option2']%>
  <%= f.select :desired_attribute, ['option1', 'option2']%>
  <%= f.label :client_category_param %>
  <%= f.select :client_category_param, ['option1', 'option2']%>
  <%= f.select :desired_attribute, ['option1', 'option2']%>
  <%= f.label :client_ethnicity_param %>
  <%= f.select :client_ethnicity_param, ['option1', 'option2']%>
  <%= f.select :desired_attribute, ['option1', 'option2']%>

  <%= f.hidden_field :organization_id, value: organization.id %>

  <%= f.button :submit %>
<% end %>