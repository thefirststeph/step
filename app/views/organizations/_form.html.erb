<%= form_for @organization do |f| %>
  
  <p> 
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <p> 
    <%= f.label :contact %><br>
    <%= f.text_field :contact %>
  </p>

   <p> 
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </p>

  <p> 
    <%= f.label :phone %><br>
    <%= f.text_field :phone %>
  </p>

  <p> 
    <%= f.label :username %><br>
    <%= f.text_field :username %>
  </p>

  <p> 
    <%= f.label :notes %><br>
    <%= f.text_area :notes %>
  </p>

  <p> 
    <%= f.label :image_url %><br>
    <%= f.text_field :image_url %>
  </p>

  <p> 
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </p>

  <%= f.button :submit %>

<% end %>